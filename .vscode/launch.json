  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Album UI via NPM",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start"],
      "env": {
        "NODE_ENV": "development",
        "BACKGROUND_COLOR": "black",
        "API_BASE_URL": "http://localhost"
      }
    }
  ]
}
